  },
  "dependencies": {
    "@ant-design/icons": "^5.0.1",
    "@iconscout/react-unicons": "^2.0.2",
    "@types/electron": "^1.6.10",
    "antd": "^5.4.0",
    "dayjs": "^1.11.7",
    "electron-packager": "^17.1.1",
    "electron-updater": "^5.3.0",
    "framer-motion": "^10.10.0",
    "moment": "^2.29.4",
    "react-apexcharts": "^1.4.0",
    "react-circular-progressbar": "^2.1.0",
